num = { "S" ~ "0" }
arith = { "+" | "*" }
var = { 'a'..'z' ~ "'"* }
expr = { num | var | "S" ~ expr | "(" ~ expr ~ arith ~ expr ~ ")" }
quant = { "~"* ~ ("A" | "E") ~ var ~ ":" }
logical = { "&" | "|" | ">" }
simple = { expr ~ "=" ~ expr }
formula = { simple | quant ~ formula | "[" ~ formula ~ logical ~ formula ~ "]" }