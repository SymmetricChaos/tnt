num = { "S"* ~ "0" }
var = { 'a'..'z' ~ "'"* }
arith = _{ "+" | "*" }
expr = _{ num | var | "S"* ~ var | "S"* ~ "(" ~ expr ~ arith ~ expr ~ ")" }
quant = { "~"* ~ ("A" | "E") ~ var ~ ":" }
logical = _{ "&" | "|" | ">" }
formula = _{ quant* ~ expr ~ "=" ~ expr | quant* ~ "[" ~ formula ~ logical ~ formula ~ "]" }